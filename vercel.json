{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ], 
  "functions": {
    "api/**/*.js": {
      "runtime": "vercel-node@33.4.1"
    },
    "api/**/*.py": {
      "runtime": "vercel-python@3.8"
    }
  }
}
